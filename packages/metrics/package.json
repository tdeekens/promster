{
  "name": "@promster/metrics",
  "version": "4.1.3",
  "description": "Metrics utilities used by all other server integrations",
  "main": "dist/modules/index.js",
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist/** && copyfiles package.json dist",
    "build": "cross-env tsc -p tsconfig.json --outDir dist --declarationDir dist/typings",
    "postbuild": "cross-env mv dist/typings/modules/* dist/typings/"
  },
  "files": [
    "readme.md",
    "package.json",
    "LICENSE",
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9",
    "npm": ">=3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdeekens/promster.git"
  },
  "author": "Tobias Deekens <nerd@tdeekens.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdeekens/promster/issues"
  },
  "homepage": "https://github.com/tdeekens/promster#readme",
  "keywords": [
    "metrics",
    "continousdelivery",
    "prometheus"
  ],
  "dependencies": {
    "lodash.memoize": "4.1.2",
    "lodash.once": "4.1.1",
    "merge-options": "2.0.0",
    "optional": "0.1.4",
    "url": "0.11.0",
    "url-value-parser": "2.0.1",
    "ts-essentials": "6.0.4"
  },
  "devDependencies": {
    "@promster/types": "0.0.0",
    "@types/lodash": "4.14.150",
    "prom-client": "12.0.0"
  },
  "optionalDependencies": {
    "gc-stats": "1.4.0"
  },
  "peerDependencies": {
    "prom-client": "12.0.0"
  }
}
